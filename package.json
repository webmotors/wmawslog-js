{
  "name": "wmawslog-js",
  "version": "1.0.0",
  "description": "Lib to log AWS Lambda logs on AWS SQS",
  "main": "index.js",
  "homepage": "https://github.com/webmotors/wmawslog-js",
  "scripts": {
    "test": "jest --coverage --detectOpenHandles --forceExit"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "keywords": [],
  "author": "Rodrigo Alviani",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.765.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.2",
    "jest-sonar-reporter": "^2.0.0"
  }
}
